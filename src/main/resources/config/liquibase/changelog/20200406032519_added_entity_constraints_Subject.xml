<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Subject.
    -->
    <changeSet id="20200406032519-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="subjects_id"
                                 baseTableName="subject_course"
                                 constraintName="fk_subject_course_subjects_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subject"/>
        <addForeignKeyConstraint baseColumnNames="courses_id"
                                 baseTableName="subject_course"
                                 constraintName="fk_subject_course_courses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>
        
    </changeSet>
</databaseChangeLog>
